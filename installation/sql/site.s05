/**ABDB**/CREATE TABLE `#__content_rating` (
  `content_id` int NOT NULL DEFAULT '0',
  `rating_sum` int unsigned NOT NULL DEFAULT '0',
  `rating_count` int unsigned NOT NULL DEFAULT '0',
  `lastip` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  PRIMARY KEY (`content_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/**ABDB**/CREATE TABLE `#__content_frontpage` (
  `content_id` int NOT NULL DEFAULT '0',
  `ordering` int NOT NULL DEFAULT '0',
  `featured_up` datetime DEFAULT NULL,
  `featured_down` datetime DEFAULT NULL,
  PRIMARY KEY (`content_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/**ABDB**/CREATE TABLE `#__content` (
  `id` int unsigned NOT NULL AUTO_INCREMENT,
  `asset_id` int unsigned NOT NULL DEFAULT '0' COMMENT 'FK to the #__assets table.',
  `title` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  `alias` varchar(400) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NOT NULL DEFAULT '',
  `introtext` mediumtext COLLATE utf8mb4_unicode_ci NOT NULL,
  `fulltext` mediumtext COLLATE utf8mb4_unicode_ci NOT NULL,
  `state` tinyint NOT NULL DEFAULT '0',
  `catid` int unsigned NOT NULL DEFAULT '0',
  `created` datetime NOT NULL,
  `created_by` int unsigned NOT NULL DEFAULT '0',
  `created_by_alias` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  `modified` datetime NOT NULL,
  `modified_by` int unsigned NOT NULL DEFAULT '0',
  `checked_out` int unsigned DEFAULT NULL,
  `checked_out_time` datetime DEFAULT NULL,
  `publish_up` datetime DEFAULT NULL,
  `publish_down` datetime DEFAULT NULL,
  `images` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `urls` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `attribs` varchar(5120) COLLATE utf8mb4_unicode_ci NOT NULL,
  `version` int unsigned NOT NULL DEFAULT '1',
  `ordering` int NOT NULL DEFAULT '0',
  `metakey` text COLLATE utf8mb4_unicode_ci,
  `metadesc` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `access` int unsigned NOT NULL DEFAULT '0',
  `hits` int unsigned NOT NULL DEFAULT '0',
  `metadata` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `featured` tinyint unsigned NOT NULL DEFAULT '0' COMMENT 'Set if article is featured.',
  `language` char(7) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'The language code for the article.',
  `note` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  PRIMARY KEY (`id`),
  KEY `idx_access` (`access`),
  KEY `idx_checkout` (`checked_out`),
  KEY `idx_state` (`state`),
  KEY `idx_catid` (`catid`),
  KEY `idx_createdby` (`created_by`),
  KEY `idx_featured_catid` (`featured`,`catid`),
  KEY `idx_language` (`language`),
  KEY `idx_alias` (`alias`(191))
) ENGINE=InnoDB AUTO_INCREMENT=21 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/**ABDB**/INSERT INTO `#__content`  VALUES 
('1', '133', 'El viaje de las criptomonedas', 'el-viaje-de-las-criptomonedas', '<!DOCTYPE html>\r\n<html lang=\"es\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>CriptoMundo: Tu Puerta al Universo Cripto</title>\r\n    <style>\r\n        /* General Styles */\r\n        body {\r\n            font-family: \'Arial\', sans-serif;\r\n            margin: 0;\r\n            padding: 0;\r\n            background-color: #0d0d0d;\r\n            color: #ffffff;\r\n        }\r\n\r\n        /* Header */\r\n        header {\r\n            background: linear-gradient(135deg, #003366, #004d40);\r\n            padding: 40px 20px;\r\n            text-align: center;\r\n        }\r\n        header h1 {\r\n            font-size: 2.8em;\r\n            margin: 0;\r\n            \r\n  color: #0af; /* Azul fluorescente */\r\ntext-shadow: 0 0 5px #066, 0 0 10px #088, 0 0 15px #0af;\r\n}\r\n        \r\n        header p {\r\n            font-size: 1.2em;\r\n            color: #cfcfcf;\r\n            margin-top: 10px;\r\n\r\n        }\r\n\r\n        /* Section Styles */\r\n        .section {\r\n            padding: 40px 20px;\r\n            border-bottom: 2px solid #004d40;\r\n            display: flex;\r\n            flex-wrap: wrap;\r\n            align-items: center;\r\n        }\r\n        .section:nth-child(even) {\r\n            background-color: #1a1a1a;\r\n        }\r\n\r\n        .section-content {\r\n            flex: 1 1 50%;\r\n            padding: 20px;\r\n        }\r\n        .section-content h2 {\r\n            font-size: 2em;\r\n            margin-bottom: 15px;\r\n            color: #00e676;\r\n        }\r\n        .section-content p {\r\n            font-size: 1em;\r\n            color: #dcdcdc;\r\n            line-height: 1.6em;\r\n        }\r\n\r\n        .section-image {\r\n            flex: 1 1 50%;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n        }\r\n        .section-image img {\r\n            max-width: 90%;\r\n            border-radius: 10px;\r\n            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.5);\r\n            transition: transform 0.3s;\r\n        }\r\n        .section-image img:hover {\r\n            transform: scale(1.1);\r\n        }\r\n\r\n        /* Interactive Module */\r\n        .interactive-box {\r\n            background: #003366;\r\n            padding: 20px;\r\n            margin: 20px auto;\r\n            max-width: 600px;\r\n            border-radius: 10px;\r\n            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.5);\r\n            text-align: center;\r\n        }\r\n        .interactive-box h3 {\r\n            margin-bottom: 10px;\r\n            color: #00e676;\r\n        }\r\n        .interactive-box select {\r\n            width: 100%;\r\n            padding: 10px;\r\n            margin-top: 10px;\r\n            border: none;\r\n            border-radius: 5px;\r\n            background: #004d40;\r\n            color: #ffffff;\r\n            font-family: \'Arial\', sans-serif;\r\n        }\r\n        .interactive-box button {\r\n            margin-top: 15px;\r\n            padding: 10px 20px;\r\n            background-color: #00e676;\r\n            color: #0d0d0d;\r\n            border: none;\r\n            border-radius: 5px;\r\n            cursor: pointer;\r\n            font-size: 1em;\r\n            transition: background-color 0.3s;\r\n        }\r\n        .interactive-box button:hover {\r\n            background-color: #66ffa6;\r\n        }\r\n\r\n        /* Footer */\r\n        footer {\r\n            background: #004d40;\r\n            padding: 20px;\r\n            text-align: center;\r\n            color: #ffffff;\r\n            font-size: 0.9em;\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <!-- Header -->\r\n    <header>\r\n        <h1>El viaje de las criptomonedas</h1>\r\n        <p>Explora el fascinante universo de las criptomonedas</p>\r\n    </header>\r\n\r\n    <!-- Section 1 -->\r\n    <div class=\"section\">\r\n        <div class=\"section-content\">\r\n            <h2>¿Qué son las Criptomonedas?</h2>\r\n            <p>\r\n                Las criptomonedas son monedas digitales que utilizan tecnología blockchain para garantizar transacciones seguras, descentralizadas y transparentes. \r\n                La primera y más conocida es Bitcoin, pero existen miles de otras, como Ethereum, Cardano y Solana.\r\n            </p>\r\n        </div>\r\n        <div class=\"section-image\">\r\n            <img src=\"http://localhost/proyectofinaljoomla/images/poiuy.jpeg\" alt=\"Criptomonedas\">\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Section 2 -->\r\n    <div class=\"section\">\r\n        <div class=\"section-image\">\r\n            <img src=\"http://localhost/proyectofinaljoomla/images/qwert.jpeg\" alt=\"Blockchain\">\r\n        </div>\r\n        <div class=\"section-content\">\r\n            <h2>¿Cómo funcionan?</h2>\r\n            <p>\r\n                Las criptomonedas operan en redes descentralizadas llamadas blockchains. Estas cadenas de bloques registran todas las transacciones \r\n                de forma pública y segura, eliminando la necesidad de intermediarios como bancos o gobiernos.\r\n            </p>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Section 3 -->\r\n    <div class=\"section\">\r\n        <div class=\"section-content\">\r\n            <h2>Usos y Aplicaciones</h2>\r\n            <p>\r\n                Desde pagos internacionales hasta inversiones y contratos inteligentes, las criptomonedas tienen una amplia gama de aplicaciones. \r\n                Incluso están revolucionando industrias como el arte digital, las finanzas y la logística.\r\n            </p>\r\n        </div>\r\n        <div class=\"section-image\">\r\n            <img src=\"http://localhost/proyectofinaljoomla/images/logistic.jpeg\" alt=\"Usos de las criptomonedas\">\r\n        </div>\r\n    </div>\r\n\r\n</body>\r\n</html>', '', '1', '24', '2024-12-26 17:23:37', '209', '', '2025-01-12 17:21:38', '209', NULL, NULL, '2024-12-26 17:23:37', NULL, '{\"image_intro\":\"\",\"image_intro_alt\":\"\",\"float_intro\":\"\",\"image_intro_caption\":\"\",\"image_fulltext\":\"\",\"image_fulltext_alt\":\"\",\"float_fulltext\":\"\",\"image_fulltext_caption\":\"\"}', '{\"urla\":\"\",\"urlatext\":\"\",\"targeta\":\"\",\"urlb\":\"\",\"urlbtext\":\"\",\"targetb\":\"\",\"urlc\":\"\",\"urlctext\":\"\",\"targetc\":\"\"}', '{\"helix_ultimate_image\":\"\",\"helix_ultimate_image_alt_txt\":\"\",\"helix_ultimate_article_format\":\"standard\",\"helix_ultimate_audio\":\"\",\"helix_ultimate_gallery\":\"\",\"helix_ultimate_video\":\"\"}', '93', '1', '', '', '1', '1033', '{\"robots\":\"\",\"author\":\"\",\"rights\":\"\"}', '0', '*', ''),
