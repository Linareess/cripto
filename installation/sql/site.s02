('256', 'com_content.article.11', '', '2025-01-10 11:42:13', '208', '11969', '1cec45a26223625ae5833f90a1c4af691d83bb07', '{\"id\":\"11\",\"asset_id\":180,\"title\":\"\\u00bfFuturo Crypto?\",\"alias\":\"futuro-crypto\",\"introtext\":\"<!DOCTYPE html>\\r\\n<html lang=\\\"es\\\">\\r\\n<head>\\r\\n    <meta charset=\\\"UTF-8\\\">\\r\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\r\\n    <title>El Futuro de las Criptomonedas: De la Revoluci\\u00f3n Digital al Cambio Global<\\/title>\\r\\n    <style>\\r\\n        \\/* General Styles *\\/\\r\\n        body {\\r\\n            font-family: \'Arial\', sans-serif;\\r\\n            margin: 0;\\r\\n            padding: 0;\\r\\n            background-color: #0d0d0d;\\r\\n            color: #ffffff;\\r\\n            overflow-x: hidden;\\r\\n        }\\r\\n\\r\\n        \\/* Header *\\/\\r\\n        header {\\r\\n            background: linear-gradient(135deg, #003366, #004d40);\\r\\n            padding: 50px 20px;\\r\\n            text-align: center;\\r\\n            animation: fadeIn 2s ease-in;\\r\\n        }\\r\\n        header h1 {\\r\\n            font-size: 1.7em;\\r\\n            margin: 0;\\r\\n            color: #00e676;\\r\\n  \\r\\n            letter-spacing: 2px;\\r\\n        }\\r\\n        header p {\\r\\n            font-size: 1.0em;\\r\\n            color: #cfcfcf;\\r\\n            margin-top: 10px;\\r\\n            letter-spacing: 1px;\\r\\n        }\\r\\n\\r\\n        \\/* Animations *\\/\\r\\n        @keyframes fadeIn {\\r\\n            0% { opacity: 0; }\\r\\n            100% { opacity: 1; }\\r\\n        }\\r\\n\\r\\n        \\/* Section Styles *\\/\\r\\n        .section {\\r\\n            padding: 40px 20px;\\r\\n            border-bottom: 2px solid #004d40;\\r\\n            display: flex;\\r\\n            flex-wrap: wrap;\\r\\n            align-items: center;\\r\\n            justify-content: center;\\r\\n            background-color: #1a1a1a;\\r\\n            animation: slideIn 1s ease-out;\\r\\n        }\\r\\n        .section:nth-child(even) {\\r\\n            background-color: #111;\\r\\n        }\\r\\n\\r\\n        .section-content {\\r\\n            flex: 1 1 45%;\\r\\n            padding: 20px;\\r\\n            margin: 10px;\\r\\n        }\\r\\n        .section-content h2 {\\r\\n            font-size: 1.0em;\\r\\n            margin-bottom: 15px;\\r\\n            color: #00e676;\\r\\n            text-transform: uppercase;\\r\\n        }\\r\\n        .section-content p {\\r\\n            font-size: 0.9em;\\r\\n            color: #dcdcdc;\\r\\n            line-height: 1.7em;\\r\\n        }\\r\\n\\r\\n        .section-content ul {\\r\\n            color: #dcdcdc;\\r\\n            padding-left: 20px;\\r\\n            list-style-type: square;\\r\\n        }\\r\\n\\r\\n        .section-image {\\r\\n            flex: 1 1 45%;\\r\\n            display: flex;\\r\\n            justify-content: center;\\r\\n            align-items: center;\\r\\n            height: 250px;\\r\\n            background-color: #003366;\\r\\n            border-radius: 15px;\\r\\n            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.5);\\r\\n        }\\r\\n        .section-image span {\\r\\n            color: #ffffff;\\r\\n            font-size: 1.0em;\\r\\n            font-weight: bold;\\r\\n            text-align: center;\\r\\n        }\\r\\n\\r\\n        \\/* Interactive Module *\\/\\r\\n        .interactive-box {\\r\\n            background: #003366;\\r\\n            padding: 30px;\\r\\n            margin: 40px auto;\\r\\n            max-width: 700px;\\r\\n            border-radius: 10px;\\r\\n            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.5);\\r\\n            text-align: center;\\r\\n            animation: bounceIn 1s ease-out;\\r\\n        }\\r\\n        .interactive-box h3 {\\r\\n            margin-bottom: 20px;\\r\\n            color: #00e676;\\r\\n            font-size: 1.0em;\\r\\n        }\\r\\n        .interactive-box select {\\r\\n            width: 100%;\\r\\n            padding: 15px;\\r\\n            margin-top: 10px;\\r\\n            border: none;\\r\\n            border-radius: 5px;\\r\\n            background: #004d40;\\r\\n            color: #ffffff;\\r\\n            font-size: 1.0em;\\r\\n        }\\r\\n        .interactive-box button {\\r\\n            margin-top: 20px;\\r\\n            padding: 12px 25px;\\r\\n            background-color: #00e676;\\r\\n            color: #0d0d0d;\\r\\n            border: none;\\r\\n            border-radius: 5px;\\r\\n            cursor: pointer;\\r\\n            font-size: 1.0em;\\r\\n            transition: background-color 0.3s;\\r\\n        }\\r\\n        .interactive-box button:hover {\\r\\n            background-color: #66ffa6;\\r\\n        }\\r\\n\\r\\n        \\/* Footer *\\/\\r\\n        footer {\\r\\n            background: #004d40;\\r\\n            padding: 20px;\\r\\n            text-align: center;\\r\\n            color: #ffffff;\\r\\n            font-size: 0.9em;\\r\\n        }\\r\\n\\r\\n        \\/* Animations *\\/\\r\\n        @keyframes slideIn {\\r\\n            0% { transform: translateX(-100%); }\\r\\n            100% { transform: translateX(0); }\\r\\n        }\\r\\n        @keyframes bounceIn {\\r\\n            0% { transform: scale(0.8); opacity: 0; }\\r\\n            50% { transform: scale(1.1); opacity: 1; }\\r\\n            100% { transform: scale(1); opacity: 1; }\\r\\n        }\\r\\n\\r\\n    <\\/style>\\r\\n<\\/head>\\r\\n<body>\\r\\n    <!-- Header -->\\r\\n    <header>\\r\\n        <h1>El Futuro de las Criptomonedas<\\/h1>\\r\\n        <p>Revoluci\\u00f3n Digital<\\/p>\\r\\n    <\\/header>\\r\\n\\r\\n    <!-- Section 1: El Presente de las Criptomonedas -->\\r\\n    <div class=\\\"section\\\">\\r\\n        <div class=\\\"section-content\\\">\\r\\n            <h2>Contexto Actual<\\/h2>\\r\\n            <p>\\r\\n                Las criptomonedas han pasado de ser una curiosidad tecnol\\u00f3gica a convertirse en una parte fundamental de la econom\\u00eda digital global. El Bitcoin, creado en 2009, marc\\u00f3 el comienzo de una nueva era en la que las monedas digitales funcionan sin la necesidad de un banco central, bas\\u00e1ndose en la tecnolog\\u00eda blockchain para garantizar la seguridad y la transparencia, actualmente muchos proyectos estan en busca de la solucion al trilema; Escalabilidad, Descentralizaci\\u00f3n y Seguridad, tener las 3 en una sola, es pr\\u00e1cticamente imposible.\\r\\n            <\\/p>\\r\\n            <p>\\r\\n                En la actualidad, las criptomonedas son utilizadas para realizar pagos internacionales, inversiones especulativas, y en aplicaciones descentralizadas que permiten la creaci\\u00f3n de contratos inteligentes, entre otros. El mercado est\\u00e1 en constante evoluci\\u00f3n, con miles de criptomonedas en circulaci\\u00f3n, cada una con un prop\\u00f3sito y caracter\\u00edsticas \\u00fanicas.\\r\\n            <\\/p>\\r\\n        <\\/div>\\r\\n        <div class=\\\"section-image\\\">\\r\\n            <span>Espacio para futuras ilustraciones sobre el presente de las criptomonedas<\\/span>\\r\\n        <\\/div>\\r\\n    <\\/div>\\r\\n\\r\\n    <!-- Section 2: El Futuro de las Criptomonedas -->\\r\\n    <div class=\\\"section\\\">\\r\\n        <div class=\\\"section-image\\\">\\r\\n            <span>Espacio para futuras ilustraciones sobre el futuro de las criptomonedas<\\/span>\\r\\n        <\\/div>\\r\\n        <div class=\\\"section-content\\\">\\r\\n            <h2>El Futuro de las Criptomonedas<\\/h2>\\r\\n            <p>\\r\\n                El futuro de las criptomonedas es prometedor, pero tambi\\u00e9n est\\u00e1 lleno de desaf\\u00edos. Se espera que las criptomonedas y la tecnolog\\u00eda blockchain contin\\u00faen evolucionando, con una mayor adopci\\u00f3n por parte de empresas, gobiernos y consumidores.\\r\\n            <\\/p>\\r\\n            <p>\\r\\n                Algunos de los principales avances que podr\\u00edan marcar el futuro incluyen:\\r\\n            <\\/p>\\r\\n            <ul>\\r\\n                <li><strong>Adopci\\u00f3n masiva:<\\/strong> El uso de criptomonedas podr\\u00eda extenderse a\\u00fan m\\u00e1s, especialmente con la creaci\\u00f3n de monedas digitales centralizadas por parte de gobiernos (CBDC).<\\/li>\\r\\n                <li><strong>Escalabilidad:<\\/strong> Soluciones como la \\\"Lightning Network\\\" podr\\u00edan mejorar la velocidad y el costo de las transacciones, haciendo a Bitcoin m\\u00e1s viable como moneda de uso diario.<\\/li>\\r\\n                <li><strong>Interoperabilidad:<\\/strong> Las criptomonedas de diferentes blockchains podr\\u00e1n comunicarse entre s\\u00ed de manera m\\u00e1s eficiente, abriendo nuevas posibilidades para aplicaciones descentralizadas.<\\/li>\\r\\n            <\\/ul>\\r\\n        <\\/div>\\r\\n    <\\/div>\\r\\n\\r\\n    <!-- Section 3: Posibles Escenarios del Futuro -->\\r\\n    <div class=\\\"section\\\">\\r\\n        <div class=\\\"section-content\\\">\\r\\n            <h2>Posibles Escenarios<\\/h2>\\r\\n            <p>\\r\\n                Si bien el futuro de las criptomonedas parece brillante, tambi\\u00e9n es incierto debido a la naturaleza vol\\u00e1til de los mercados y la incertidumbre regulatoria. A continuaci\\u00f3n, se presentan algunos posibles escenarios:\\r\\n            <\\/p>\\r\\n            <ul>\\r\\n                <li><strong>Integraci\\u00f3n Global:<\\/strong> Las criptomonedas podr\\u00edan convertirse en una moneda globalmente aceptada, permitiendo pagos y transacciones a nivel mundial sin las barreras de las monedas tradicionales.<\\/li>\\r\\n                <li><strong>Descentralizaci\\u00f3n de la Econom\\u00eda:<\\/strong> El control financiero podr\\u00eda pasar de los bancos a los usuarios, empoderando a individuos alrededor del mundo para gestionar sus propios fondos sin la intervenci\\u00f3n de gobiernos o instituciones financieras.<\\/li>\\r\\n                <li><strong>Regulaci\\u00f3n Estricta:<\\/strong> Algunos pa\\u00edses podr\\u00edan implementar regulaciones estrictas que limiten el uso de criptomonedas, buscando proteger sus econom\\u00edas y sistemas financieros tradicionales.<\\/li>\\r\\n            <\\/ul>\\r\\n        <\\/div>\\r\\n        <div class=\\\"section-image\\\">\\r\\n            <span>Espacio para futuras ilustraciones sobre los posibles escenarios de las criptomonedas<\\/span>\\r\\n        <\\/div>\\r\\n    <\\/div>\\r\\n\\r\\n    <!-- Interactive Section -->\\r\\n    <div class=\\\"interactive-box\\\">\\r\\n        <h3>Explora m\\u00e1s sobre el futuro de las criptomonedas<\\/h3>\\r\\n        <select>\\r\\n            <option>Selecciona un tema...<\\/option>\\r\\n            <option>El Presente de las Criptomonedas<\\/option>\\r\\n            <option>El Futuro de las Criptomonedas<\\/option>\\r\\n            <option>Posibles Escenarios<\\/option>\\r\\n        <\\/select>\\r\\n        <button>Descubrir<\\/button>\\r\\n    <\\/div>\\r\\n\\r\\n\\r\\n<\\/body>\\r\\n<\\/html>\\r\\n\",\"fulltext\":\"\",\"state\":\"1\",\"catid\":18,\"created\":\"2025-01-05 16:45:42\",\"created_by\":\"208\",\"created_by_alias\":\"\",\"modified\":\"2025-01-10 11:42:13\",\"modified_by\":208,\"checked_out\":208,\"checked_out_time\":\"2025-01-10 11:36:46\",\"publish_up\":\"2025-01-05 16:45:42\",\"publish_down\":null,\"images\":\"{\\\"image_intro\\\":\\\"\\\",\\\"image_intro_alt\\\":\\\"\\\",\\\"float_intro\\\":\\\"\\\",\\\"image_intro_caption\\\":\\\"\\\",\\\"image_fulltext\\\":\\\"\\\",\\\"image_fulltext_alt\\\":\\\"\\\",\\\"float_fulltext\\\":\\\"\\\",\\\"image_fulltext_caption\\\":\\\"\\\"}\",\"urls\":\"{\\\"urla\\\":\\\"\\\",\\\"urlatext\\\":\\\"\\\",\\\"targeta\\\":\\\"\\\",\\\"urlb\\\":\\\"\\\",\\\"urlbtext\\\":\\\"\\\",\\\"targetb\\\":\\\"\\\",\\\"urlc\\\":\\\"\\\",\\\"urlctext\\\":\\\"\\\",\\\"targetc\\\":\\\"\\\"}\",\"attribs\":\"{\\\"article_layout\\\":\\\"\\\",\\\"show_title\\\":\\\"\\\",\\\"link_titles\\\":\\\"\\\",\\\"show_tags\\\":\\\"\\\",\\\"show_intro\\\":\\\"\\\",\\\"info_block_position\\\":\\\"\\\",\\\"info_block_show_title\\\":\\\"\\\",\\\"show_category\\\":\\\"\\\",\\\"link_category\\\":\\\"\\\",\\\"show_parent_category\\\":\\\"\\\",\\\"link_parent_category\\\":\\\"\\\",\\\"show_author\\\":\\\"\\\",\\\"link_author\\\":\\\"\\\",\\\"show_create_date\\\":\\\"\\\",\\\"show_modify_date\\\":\\\"\\\",\\\"show_publish_date\\\":\\\"\\\",\\\"show_item_navigation\\\":\\\"\\\",\\\"show_hits\\\":\\\"\\\",\\\"show_noauth\\\":\\\"\\\",\\\"urls_position\\\":\\\"\\\",\\\"alternative_readmore\\\":\\\"\\\",\\\"article_page_title\\\":\\\"\\\",\\\"show_publishing_options\\\":\\\"\\\",\\\"show_article_options\\\":\\\"\\\",\\\"show_urls_images_backend\\\":\\\"\\\",\\\"show_urls_images_frontend\\\":\\\"\\\",\\\"helix_ultimate_image\\\":\\\"\\\",\\\"helix_ultimate_image_alt_txt\\\":\\\"\\\",\\\"helix_ultimate_article_format\\\":\\\"standard\\\",\\\"helix_ultimate_audio\\\":\\\"\\\",\\\"helix_ultimate_gallery\\\":\\\"\\\",\\\"helix_ultimate_video\\\":\\\"\\\"}\",\"version\":12,\"ordering\":0,\"metakey\":\"\",\"metadesc\":\"\",\"access\":1,\"hits\":17,\"metadata\":\"{\\\"robots\\\":\\\"\\\",\\\"author\\\":\\\"\\\",\\\"rights\\\":\\\"\\\"}\",\"featured\":\"0\",\"language\":\"*\",\"note\":\"\"}', '0'),
('257', 'com_content.article.17', '', '2025-01-10 11:42:32', '208', '7058', 'f50b55eecf8334f1568e4e184e265dde8d19d4ae', '{\"id\":\"17\",\"asset_id\":203,\"title\":\"De heroes a villanos\",\"alias\":\"de-heroes-a-villanos\",\"introtext\":\"<!DOCTYPE html>\\r\\n<html lang=\\\"es\\\">\\r\\n<head>\\r\\n    <meta charset=\\\"UTF-8\\\">\\r\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\r\\n    <title>De H\\u00e9roes a Villanos en Criptomonedas<\\/title>\\r\\n    <style>\\r\\n        \\/* General Styles *\\/\\r\\n        body {\\r\\n            font-family: \'Arial\', comic-sans;\\r\\n            margin: 0;\\r\\n            padding: 0;\\r\\n            background-color: #0d0d0d;\\r\\n            color: #ffffff;\\r\\n        }\\r\\n\\r\\n        \\/* Header *\\/\\r\\n        header {\\r\\n            background: linear-gradient(135deg, #003366, #004d40);\\r\\n            padding: 40px 20px;\\r\\n            text-align: center;\\r\\n        }\\r\\n        header h1 {\\r\\n            font-size: 1.3em;\\r\\n            margin: 0;\\r\\n            color: #00e676;\\r\\n        }\\r\\n        header p {\\r\\n            font-size: 1.0em;\\r\\n            color: #cfcfcf;\\r\\n            margin-top: 10px;\\r\\n        }\\r\\n\\r\\n        \\/* Article Styles *\\/\\r\\n        .article-content {\\r\\n            padding: 40px 20px;\\r\\n        }\\r\\n        .article-content h2 {\\r\\n            font-size: 1.1em;\\r\\n            color: #00e676;\\r\\n        }\\r\\n        .article-content p {\\r\\n            font-size: 0.9em;\\r\\n            line-height: 1.6;\\r\\n            color: #dcdcdc;\\r\\n            margin-bottom: 20px;\\r\\n        }\\r\\n\\r\\n        .section {\\r\\n            display: flex;\\r\\n            flex-wrap: wrap;\\r\\n            margin-bottom: 40px;\\r\\n        }\\r\\n\\r\\n        .section-text {\\r\\n            width: 100%;\\r\\n        }\\r\\n\\r\\n        \\/* Footer *\\/\\r\\n        footer {\\r\\n            background: #004d40;\\r\\n            padding: 20px;\\r\\n            text-align: center;\\r\\n            color: #ffffff;\\r\\n            font-size: 0.9em;\\r\\n        }\\r\\n    <\\/style>\\r\\n<\\/head>\\r\\n<body>\\r\\n    <!-- Header -->\\r\\n    <header>\\r\\n        <h1>De H\\u00e9roes a Villanos: Criptomonedas y sus Controversias<\\/h1>\\r\\n        <p>Casos de figuras y proyectos que cayeron en desgracia en el mundo cripto<\\/p>\\r\\n    <\\/header>\\r\\n\\r\\n    <!-- Article Content -->\\r\\n    <div class=\\\"article-content\\\">\\r\\n        <p>En el mundo de las criptomonedas, algunos de los l\\u00edderes y proyectos m\\u00e1s prominentes han enfrentado ca\\u00eddas de imagen debido a estafas, fraudes y problemas legales. Aqu\\u00ed exploramos brevemente algunos de los casos m\\u00e1s notorios.<\\/p>\\r\\n\\r\\n        <!-- Section 1: Arthur Hayes -->\\r\\n        <div class=\\\"section\\\">\\r\\n          \\r\\n            <div class=\\\"section-text\\\">\\r\\n                <h2>Arthur Hayes - BitMEX<\\/h2>\\r\\n                <p>Arthur Hayes, cofundador de BitMEX, fue acusado de operar una plataforma de intercambio sin licencias adecuadas, involucr\\u00e1ndose en violaciones de leyes financieras. Su plataforma permiti\\u00f3 el apalancamiento excesivo, lo que contribuy\\u00f3 a la volatilidad extrema en los mercados de criptomonedas.<\\/p>\\r\\n            <\\/div>\\r\\n        <\\/div>\\r\\n\\r\\n        <!-- Section 2: Justin Sun -->\\r\\n        <div class=\\\"section\\\">\\r\\n     \\r\\n            <div class=\\\"section-text\\\">\\r\\n                <h2>Justin Sun - TRON<\\/h2>\\r\\n                <p>Fundador de TRON, Sun fue criticado por inflar artificialmente el valor de su criptomoneda a trav\\u00e9s de compras masivas y estrategias de marketing cuestionables. Su intento de adquirir una cena con Warren Buffett tambi\\u00e9n fue percibido como una t\\u00e1ctica publicitaria poco \\u00e9tica.<\\/p>\\r\\n            <\\/div>\\r\\n        <\\/div>\\r\\n\\r\\n        <!-- Section 3: Terra Luna -->\\r\\n        <div class=\\\"section\\\">\\r\\n        \\r\\n            <div class=\\\"section-text\\\">\\r\\n                <h2>Terra Luna<\\/h2>\\r\\n                <p>Terra Luna, uno de los proyectos m\\u00e1s prometedores de 2021, colaps\\u00f3 cuando su stablecoin UST perdi\\u00f3 su paridad con el d\\u00f3lar. El colapso result\\u00f3 en la p\\u00e9rdida de miles de millones de d\\u00f3lares, y su creador, Do Kwon, enfrent\\u00f3 acusaciones de fraude y manipulaci\\u00f3n del mercado.<\\/p>\\r\\n            <\\/div>\\r\\n        <\\/div>\\r\\n\\r\\n        <!-- Section 4: Bitcoin Cash -->\\r\\n        <div class=\\\"section\\\">\\r\\n       \\r\\n            <div class=\\\"section-text\\\">\\r\\n                <h2>Bitcoin Cash<\\/h2>\\r\\n                <p>Bitcoin Cash surgi\\u00f3 como una bifurcaci\\u00f3n de Bitcoin con el objetivo de hacer las transacciones m\\u00e1s r\\u00e1pidas y baratas. Sin embargo, a medida que la comunidad se dividi\\u00f3 sobre su visi\\u00f3n, la moneda fue objeto de cr\\u00edticas por la falta de adopci\\u00f3n y su uso en actividades il\\u00edcitas.<\\/p>\\r\\n            <\\/div>\\r\\n        <\\/div>\\r\\n\\r\\n        <!-- Section 5: XRP -->\\r\\n        <div class=\\\"section\\\">\\r\\n         \\r\\n            <div class=\\\"section-text\\\">\\r\\n                <h2>XRP<\\/h2>\\r\\n                <p>XRP, la criptomoneda de Ripple, ha estado envuelta en controversia debido a su disputa legal con la SEC sobre la clasificaci\\u00f3n de su token. La acusaci\\u00f3n de que XRP es un valor no registrado ha provocado que muchos lo vean como una moneda con un futuro incierto, lo que afecta su reputaci\\u00f3n.<\\/p>\\r\\n            <\\/div>\\r\\n        <\\/div>\\r\\n    <\\/div>\\r\\n\\r\\n<\\/body>\\r\\n<\\/html>\\r\\n\",\"fulltext\":\"\",\"state\":\"1\",\"catid\":17,\"created\":\"2025-01-06 20:55:02\",\"created_by\":\"209\",\"created_by_alias\":\"\",\"modified\":\"2025-01-10 11:42:32\",\"modified_by\":208,\"checked_out\":208,\"checked_out_time\":\"2025-01-10 11:42:15\",\"publish_up\":\"2025-01-06 20:55:02\",\"publish_down\":null,\"images\":\"{\\\"image_intro\\\":\\\"\\\",\\\"image_intro_alt\\\":\\\"\\\",\\\"float_intro\\\":\\\"\\\",\\\"image_intro_caption\\\":\\\"\\\",\\\"image_fulltext\\\":\\\"\\\",\\\"image_fulltext_alt\\\":\\\"\\\",\\\"float_fulltext\\\":\\\"\\\",\\\"image_fulltext_caption\\\":\\\"\\\"}\",\"urls\":\"{\\\"urla\\\":\\\"\\\",\\\"urlatext\\\":\\\"\\\",\\\"targeta\\\":\\\"\\\",\\\"urlb\\\":\\\"\\\",\\\"urlbtext\\\":\\\"\\\",\\\"targetb\\\":\\\"\\\",\\\"urlc\\\":\\\"\\\",\\\"urlctext\\\":\\\"\\\",\\\"targetc\\\":\\\"\\\"}\",\"attribs\":\"{\\\"article_layout\\\":\\\"\\\",\\\"show_title\\\":\\\"\\\",\\\"link_titles\\\":\\\"\\\",\\\"show_tags\\\":\\\"\\\",\\\"show_intro\\\":\\\"\\\",\\\"info_block_position\\\":\\\"\\\",\\\"info_block_show_title\\\":\\\"\\\",\\\"show_category\\\":\\\"\\\",\\\"link_category\\\":\\\"\\\",\\\"show_parent_category\\\":\\\"\\\",\\\"link_parent_category\\\":\\\"\\\",\\\"show_author\\\":\\\"\\\",\\\"link_author\\\":\\\"\\\",\\\"show_create_date\\\":\\\"\\\",\\\"show_modify_date\\\":\\\"\\\",\\\"show_publish_date\\\":\\\"\\\",\\\"show_item_navigation\\\":\\\"\\\",\\\"show_hits\\\":\\\"\\\",\\\"show_noauth\\\":\\\"\\\",\\\"urls_position\\\":\\\"\\\",\\\"alternative_readmore\\\":\\\"\\\",\\\"article_page_title\\\":\\\"\\\",\\\"show_publishing_options\\\":\\\"\\\",\\\"show_article_options\\\":\\\"\\\",\\\"show_urls_images_backend\\\":\\\"\\\",\\\"show_urls_images_frontend\\\":\\\"\\\",\\\"helix_ultimate_image\\\":\\\"\\\",\\\"helix_ultimate_image_alt_txt\\\":\\\"\\\",\\\"helix_ultimate_article_format\\\":\\\"standard\\\",\\\"helix_ultimate_audio\\\":\\\"\\\",\\\"helix_ultimate_gallery\\\":\\\"\\\",\\\"helix_ultimate_video\\\":\\\"\\\"}\",\"version\":9,\"ordering\":0,\"metakey\":\"\",\"metadesc\":\"\",\"access\":1,\"hits\":18,\"metadata\":\"{\\\"robots\\\":\\\"\\\",\\\"author\\\":\\\"\\\",\\\"rights\\\":\\\"\\\"}\",\"featured\":\"0\",\"language\":\"*\",\"note\":\"\"}', '0'),
